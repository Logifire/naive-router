{
    "name": "logifire/naive-router",
    "description": "Router implementation using PSR-7",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.0",
        "psr/container": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "NaiveRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "NaiveRouter\\Tests\\": "tests/" 
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.1",
        "nyholm/psr7": "1.0.1",
        "logifire/naive-container": "1.0.0"
    }
}
